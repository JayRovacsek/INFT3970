@model IEnumerable<INFT3970Project.Models.TemperatureModel>

@{
    ViewData["Title"] = "Temperature";
}

<h2>Temperature</h2>

@* Examples of how the IEnummerable model can be used are: *@

@{ 
    foreach(var m in Model)
    {
        <h3>@m.Id</h3>
        <p>The temperature logged was: @m.Temperature</p>
        <p>The time this was logged was: @m.Timestamp</p>
    }
}